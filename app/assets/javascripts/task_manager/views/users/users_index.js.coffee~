class Crm.Views.UsersIndex extends Backbone.View

  template: JST['users/index']
  tagName: "select"
  id: "new_task_id"
  
  initialize: ->
    @collection = new Crm.Collections.Users()
    @collection.fetch()
    @collection.on('reset', @render, this)
  
  render: ->
    $(@el).html(@template(users: @collection))
    this
